{
  "name": "FullKetoReact",
  "version": "1.0.0",
  "description": "Application dedicated to the Ketogenic lifestyle made with React",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build", 
    "test": "mocha --timeout 10000"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joverlee521/FullKeto_React.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/joverlee521/FullKeto_React/issues"
  },
  "homepage": "https://github.com/joverlee521/FullKeto_React#readme",
  "dependencies": {
    "@material-ui/core": "^3.9.2",
    "concurrently": "^4.1.0",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "if-env": "^1.0.4",
    "mysql2": "^1.6.5",
    "nodemon": "^1.18.10",
    "path": "^0.12.7",
    "request": "^2.88.0",
    "sequelize": "^4.42.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "mocha": "^5.2.0"
  }
}
